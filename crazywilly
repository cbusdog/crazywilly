# Create a new user account
New-ADUser -Name "CrazyWilly2" -SamAccountName "CrazyWilly2" -UserPrincipalName "CrazyWilly2@corp.m1.com" -Path "CN=Users,DC=corp,DC=m1,DC=com" -AccountPassword (ConvertTo-SecureString "StrongPassword!" -AsPlainText -Force) -Enabled $true

# Enable the user account (if it wasn't enabled during creation)
Enable-ADAccount -Identity "CrazyWilly2"
